<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

      <record id="wb_student_tree_view" model="ir.ui.view">
          <field name="name">wb.student.tree.view</field>
          <field name="model">wb.student</field>
          <field name="arch" type="xml">
              <tree>
                  <field name="name" />
                  <field name="roll_no" />
                  <field name = "country_id"/>
<!--                  <field name="name1" />-->
<!--                  <field name="is_paid" />-->
              </tree>
          </field>
      </record>

      <record id="wb_student_form_view" model="ir.ui.view">
          <field name="name">wb.student.form.view</field>
          <field name="model">wb.student</field>
          <field name="arch" type="xml">
              <form>
                  <group>
                      <group>
                          <field name="name" />
<!--                          <field name="name1" />-->
<!--                          <field name="name2" />-->
<!--                          <field name="name3" />-->
<!--                          <field name="name4" />-->
                          <field name="roll_no" />
                      </group>
                      <group>
                          <field name="address" />
                          <field name="country_id" />
                          <field name="state_id" />
<!--                          <field name="address_html" />-->
<!--                          <field name="std_fees" />-->
<!--                          <field name="is_paid" />-->
<!--                          <field name="gender" placeholder="please select gender" />-->
<!--                          <field name="advance_gender" />-->
<!--                          <field name="school_data" />-->
<!--                          <field name="join_date" />-->
                      </group>
                  </group>
              </form>
          </field>
      </record>

      <record id="wb_student_action" model="ir.actions.act_window">
            <field name="name">Student</field>
            <field name="res_model">wb.student</field>
            <field name="view_mode">tree,form</field>
      </record>

      <menuitem id="wb_student_root_menu" name="Student" action="wb_student_action"/>
<!--      <menuitem id="wb_student_sub_menu" parent="wb_student_root_menu" name="WebLearns"/>-->
<!--      <menuitem id="wb_student_menu" parent="wb_student_sub_menu" name="Student List" action="wb_student_action"/>-->


</odoo>