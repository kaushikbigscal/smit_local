<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_matrix_attendance_sync" model="ir.cron">
            <field name="name">Matrix: Sync Attendance Records</field>
            <field name="model_id" ref="model_matrix_attendance_sync"/>
            <field name="state">code</field>
            <field name="code">model._sync_attendance()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="nextcall" eval="DateTime.now().replace(hour=18, minute=26, second=0)"/>
        </record>
    </data>
</odoo>