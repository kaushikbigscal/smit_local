<odoo>
    <record id="ir_cron_generate_amc_reminders" model="ir.cron">
        <field name="name">Generate AMC Contract Reminders</field>
        <field name="model_id" ref="task_deadline_reminder.model_task_reminder"/>
        <field name="state">code</field>
        <field name="code">model.generate_amc_end_reminders()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
        <field name="nextcall" eval="(DateTime.now() + relativedelta()).replace(hour=5, minute=30, second=0).strftime('%Y-%m-%d %H:%M:%S')" />
    </record>
</odoo>