<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data noupdate="1">
    <record id="ir_cron_process_attendance_and_day_out" model="ir.cron">
      <field name="name">Process Attendance hour_to and Day Out</field>
      <field name="model_id" ref="model_resource_calendar_attendance"/>
      <field name="state">code</field>
      <field name="code">model.process_attendance_and_day_out()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="active">True</field>
       <field name="nextcall" eval="(DateTime.now() + relativedelta(days=1)).strftime('%Y-%m-%d 23:45:00')"/>
    </record>
  </data>
</odoo>
