<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Default Categories -->
        <record id="category_general" model="custom.timesheet.category">
            <field name="name">General</field>
            <field name="code">GENERAL</field>
        </record>

        <record id="category_crm" model="custom.timesheet.category">
            <field name="name">Lead/Opportunity</field>
            <field name="code">CRM</field>
            <field name="module_name">crm</field>
            <field name="model_names">crm.lead</field>
        </record>

        <record id="category_project" model="custom.timesheet.category">
            <field name="name">Project Tasks</field>
            <field name="code">PROJECT</field>
            <field name="module_name">project</field>
            <field name="model_names">project.project,project.task</field>
        </record>

        <record id="category_field_service" model="custom.timesheet.category">
            <field name="name">Service Call</field>
            <field name="code">SERVICE_CALL</field>
            <field name="module_name">service call</field>
            <field name="model_names">project.task</field>
        </record>

    </data>
</odoo>