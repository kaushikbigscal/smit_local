<?xml version="1.0" encoding="utf-8"?>
<templates xml:space="preserve">
    <t t-name="button.ListView.Buttons" t-inherit="web.ListView.Buttons">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button name="action_create_fields" type="object" class="btn btn-primary" style="margin-left: 10px;"
                    t-on-click="action_create_fields">
                Create Fields
            </button>
        </xpath>
    </t>
</templates>