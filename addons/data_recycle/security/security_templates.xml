<odoo noupdate="0">
    <data>
        <!-- your base, empty template that should only be loaded at install time -->
        <record id="report_template_xml_upload" model="ir.ui.view">
            <field name="name">data_recycle.report_template_xml_upload</field>
            <field name="model">hr.payslip</field>
            <field name="type">qweb</field>
            <field name="arch_db"><![CDATA[
        <template id="report_template_xml_upload">
          <t t-call="web.external_layout">
            <main/>
          </t>
        </template>
      ]]></field>
        </record>

        <record id="action_xml_upload_custom_report_format_for_all" model="ir.actions.report">
            <field name="name">Custom Report</field>
            <field name="model">hr.payslip</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">data_recycle.report_template_xml_upload</field>
            <field name="report_file">data_recycle.report_template_xml_upload</field>
            <field name="print_report_name">
                '%s_%s_%s' % (object.employee_id.name.replace(" ", "") if object.employee_id else 'Employee',
                object.date_from.strftime("%B%Y") if object.date_from else 'UnknownMonth', time.strftime("%Y%m%d_%H%M"))
            </field>
        </record>


        <record id="report_template_service_call_xml_upload" model="ir.ui.view">
            <field name="name">data_recycle.report_template_service_call_xml_upload</field>
