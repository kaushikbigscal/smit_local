            <field name="print_report_name">
                'ServiceReport_%s_%s' % (
                object.partner_id.name.replace(' ', '') if object.partner_id else 'NoCustomer',
                time.strftime("%Y-%m-%d_%H_%M")
                )
            </field>
        </record>


